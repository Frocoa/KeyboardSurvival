config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Key",
"language": "GDScript",
"path": "res://Scenes/key.gd"
} ]
_global_script_class_icons={
"Key": ""
}

[application]

config/name="Keyboard Attack"
run/main_scene="res://Scenes/main_menu.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://global.gd"
MenuTheme="*res://Sounds/MenuTheme.tscn"
LevelTheme="*res://Sounds/LevelTheme.tscn"
ConfirmSound="*res://Sounds/ConfirmSound.tscn"
HitSound="*res://Sounds/HitSound.tscn"
DeathSound="*res://Sounds/DeathSound.tscn"

[display]

window/dpi/allow_hidpi=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[global]

display=false

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://new_theme.tres"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
